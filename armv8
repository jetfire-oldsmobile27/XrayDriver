[settings]
os=Linux
arch=armv8
compiler=gcc
compiler.version=12
compiler.cppstd=gnu11
compiler.libcxx=libstdc++11
build_type=Release

[buildenv]
CC=aarch64-unknown-linux-gnu-gcc --sysroot=/work/aarch64-sysroot
CXX=aarch64-unknown-linux-gnu-g++ --sysroot=/work/aarch64-sysroot
LD=/usr/xcc/aarch64-unknown-linux-gnu/bin/aarch64-unknown-linux-gnu-ld --sysroot=/work/aarch64-sysroot
AR=/usr/xcc/aarch64-unknown-linux-gnu/bin/aarch64-unknown-linux-gnu-ar
